from django_cron import CronJobBase, Schedule

class updater(CronJobBase):
    RUN_EVERY_MINS = 1 # every 2 hours

    schedule = Schedule(run_every_mins=1)
    code = 'my_app.my_cron_job'    # a unique code

    def do(self):
        pass    # do your thing here